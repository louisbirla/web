{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "yarn gql:build && next build",
    "start": "next start",
    "format": "npx eslint --fix . && npx prettier --write .",
    "gql:build": "yarn gql:reset && yarn gql:gen && npx prettier --write ./**/gql/*.*",
    "gql:gen": "apollo client:codegen gql --target typescript",
    "gql:dev": "yarn gql:gen --watch",
    "gql:reset": "rm gql/*.* & rm ./**/gql/*.* || true",
    "test": "jest",
    "test:dev": "yarn test --watch"
  },
  "dependencies": {
    "@apollo/client": "3.2.5",
    "@chakra-ui/core": "1.0.0-rc.5",
    "@sentry/react": "5.27.2",
    "graphql": "15.4.0",
    "next": "latest",
    "next-seo": "4.15.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-feather": "2.0.8",
    "react-ga": "3.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.5",
    "@testing-library/react": "11.1.2",
    "@types/jest": "26.0.15",
    "@types/node": "14.14.6",
    "@types/react": "16.9.53",
    "@types/react-dom": "16.9.9",
    "apollo": "2.31.0",
    "babel-jest": "26.5.2",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.21.4",
    "jest": "26.6.3",
    "jest-sonar-reporter": "2.0.0",
    "ts-jest": "26.4.4",
    "typescript": "4.0.5"
  },
  "license": "MIT"
}
