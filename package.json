{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "yarn gql:build && next build",
    "start": "next start",
    "format": "npx eslint --fix . && npx prettier --write .",
    "gql:build": "yarn gql:reset && yarn gql:gen && npx prettier --write ./**/gql/*.*",
    "gql:gen": "apollo client:codegen gql --target typescript ",
    "gql:dev": "yarn gql:gen --watch",
    "gql:reset": "rm gql/*.* & rm ./**/gql/*.* || true"
  },
  "dependencies": {
    "@apollo/client": "3.1.5",
    "@chakra-ui/core": "1.0.0-rc.3",
    "@sentry/react": "5.23.0",
    "graphql": "15.3.0",
    "next": "latest",
    "next-seo": "4.7.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-feather": "2.0.8",
    "react-ga": "3.1.2"
  },
  "devDependencies": {
    "@types/node": "12.12.58",
    "@types/react": "16.9.49",
    "@types/react-dom": "16.9.8",
    "apollo": "2.30.3",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.6",
    "typescript": "4.0.2"
  },
  "license": "MIT"
}
